	  $(function() {
		$("#tags").autocomplete({
		  source: availableTags
		});
	  });
	  
	  // wait for the DOM to be loaded 
		$(document).ready(function() {						
		    $( "form" ).submit(function( event ) {	
		      var found = $.inArray($("input").val(), availableTags);
		      if(found != -1){
		      	thehtml = "<table class=\"table\">"
		      	thehtml = thehtml + <td bgcolor=\"#CCFFCC\" style=\"padding:10px\"> Gmax 189: <a href=\"http://soykb.org/gene_card.php?gene=" + availableTags[found] + "\">" + availableTags[found] + "</a>"
		      	thehtml = thehtml + "<br><br>Gmax 109: <a href=\"http://soykb.org/gene_card_v1.php?gene=" + get109(availableTags[found]) + "\">" + get109(availableTags[found]) + "</a></td>"
		      	thehtml = thehtml + "<td style=\"padding:10px\">" + dict[availableTags[found]] + "</table>"
		      	$( "span" ).html(thehtml).show();
				event.preventDefault();
		      }
		      
		      else{	 
				  $( "span" ).text( "Not valid!" ).show();
				  event.preventDefault();
			  }
			});
		}); 
	  
